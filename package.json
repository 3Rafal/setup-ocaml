{
  "private": true,
  "scripts": {
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.ts\" --cache",
    "test": "jest",
    "build:copy": "mkdir -p dist && shx cp src/*.{bat,cmd,sh} dist/",
    "build:setup-ocaml": "ncc build src/setup-ocaml.ts --out=dist --license=LICENSE.txt --v8-cache",
    "build": "npm-run-all -p build:*"
  },
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/tool-cache": "1.6.1",
    "@vercel/ncc": "0.26.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.16",
    "@types/node": "14.14.10",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-simple-import-sort": "6.0.1",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "shx": "0.3.3",
    "ts-jest": "26.4.4",
    "typescript": "4.1.2"
  }
}
