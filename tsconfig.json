{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "moduleResolution": "Node",
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "ES2022"
  },
  "include": ["src"]
}
